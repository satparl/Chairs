@model ChairElections.Models.Committee

<h2>@(ViewContext.RouteData.Values["action"])</h2>

<form asp-action="@ViewContext.RouteData.Values["action"]">
    <div class="form-group">
        <label asp-for="Name"></label>
        <input asp-for="Name" class="form-control" />
    </div>
    <div class="form-group">
        <label asp-for="Status"></label>
        <input asp-for="Status" class="form-control" />
    </div>
    <div class="form-group">
        <label asp-for="AssignedParty"></label>
        <input asp-for="AssignedParty" class="form-control" />
    </div>
    <button type="submit" class="btn btn-success">Save</button>
</form>
